public void factorial(tensor<16> t, int idx) {
  if (idx == 0) {
    t@(idx) = 1;
  } else if (idx < t.dim(0)) {
    t@(idx) = t@(idx-1) * idx;
  } else {
    return;
  }
  writeln idx, "! = ", t@(idx);
  factorial(t, idx+1);
}
public int udf() {
  tensor<16> t;
  factorial(t, 0);
  writeln(t);
  return 0;
}

